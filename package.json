    "expo": "^27.0.1",
    "firebase": "^5.3.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-elements": "^0.19.1",
